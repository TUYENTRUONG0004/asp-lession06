@model IEnumerable<Tdtlession06CF.Models.TdtCategory>

@{
    ViewBag.Title = "TdtIndex";
}

<h2>TdtIndex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TdtCategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TdtCategoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TdtId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TdtId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TdtId })
        </td>
    </tr>
}

</table>
